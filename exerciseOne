function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(220);
  myTri(120, 130, 70, 24, 250, 130)
  myEllipse(200,200,50,130)
  mySquare(250,250,100)
}

function myTri(x1,x2,x3,x4,x5,x6){
  triangle(x1,x2,x3,x4,x5,x6)
}
function myEllipse(x,y,width,height)
{
  ellipse(x,y,width,height)
}
function mySquare(x,y,width,size){
  square(x,y,width,size)
}
